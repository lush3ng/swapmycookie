{
	"name": "Hack Swap My Cookies",
	"version": "0.3",
	
	"description": "If you have multiple accounts let Hack Swap My Cookies manage them for you!",
	"icons": {
		"32": "img/icon_32x32.png",
		"48": "img/icon_48x48.png",
		"128": "img/icon_128x128.png"
	},
	
	"browser_action": {
		"default_icon": "img/icon_32x32.png",
		"default_title": "Hack Swap My Cookies",
		"default_popup": "popup.html"
	},
	
	"background": {
		"scripts" : ["/lib/jquery-1.4.2.min.js", "/js/data.js", "/js/tools.js", "/js/background.js"]
	},
	
	"options_page": "options.html",
	
	"permissions": [ 
		"tabs",
		"\u003Call_urls\u003E",
		"unlimitedStorage",
		"cookies" 
	],
	
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	
	"minimum_chrome_version": "12",
	"manifest_version": 2,
	"update_url": "http://clients2.google.com/service/update2/crx"
}
